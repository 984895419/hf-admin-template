{
  "name": "hf-vue-admin-template",
  "version": "1.0.0",
  "description": "huafeng vue-admin-template",
  "author": "huafeng",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "preview": "node build/index.js --preview",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "new": "plop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "admin",
    "dashboard",
    "element-ui",
    "boilerplate",
    "admin-template",
    "management-system"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PanJiaChen/vue-element-admin.git"
  },
  "bugs": {
    "url": "https://github.com/PanJiaChen/vue-element-admin/issues"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "clipboard": "2.0.6",
    "dropzone": "5.7.0",
    "echarts": "4.6.0",
    "element-ui": "2.13.0",
    "fuse.js": "3.6.1",
    "js-cookie": "2.2.1",
    "js-md5": "^0.7.3",
    "jsonlint": "1.6.3",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "path-to-regexp": "6.1.0",
    "pubsub-js": "^1.9.4",
    "qs": "^6.10.3",
    "screenfull": "5.0.2",
    "script-loader": "0.7.2",
    "sockjs-client": "^1.5.1",
    "sortablejs": "1.10.2",
    "stompjs": "^2.3.3",
    "v-clipboard": "^2.2.3",
    "vue": "2.6.11",
    "vue-count-to": "1.0.13",
    "vue-i18n": "7.3.2",
    "vue-json-editor": "^1.4.3",
    "vue-json-views": "^1.3.0",
    "vue-router": "3.1.6",
    "vue-splitpane": "1.0.6",
    "vue-super-flow": "^1.3.8",
    "vuedraggable": "2.23.2",
    "vuex": "3.1.2",
    "xlsx": "^0.17.4"
  },
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@babel/register": "7.8.6",
    "@vue/cli-plugin-babel": "4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-plugin-unit-jest": "4.2.3",
    "@vue/cli-service": "4.2.3",
    "@vue/test-utils": "1.0.0-beta.31",
    "autoprefixer": "^9.7.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.0.1",
    "babel-jest": "25.1.0",
    "chalk": "3.0.0",
    "chokidar": "3.3.1",
    "connect": "3.7.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.3",
    "less": "^4.1.1",
    "less-loader": "^7.0.0",
    "lint-staged": "10.0.8",
    "mockjs": "1.1.0",
    "node-sass": "^4.13.1",
    "plop": "^2.7.6",
    "runjs": "^4.4.2",
